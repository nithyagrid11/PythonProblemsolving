class Solution(object):
    def longestCommonPrefix(self, strs):
        common = ""
        strs = sorted(strs)
        first = strs[0]
        last = strs[-1]
        for i in range(min(len(first), len(last))):
            if first[i] != last[i]:
                return common
            common += first[i]
        return common
